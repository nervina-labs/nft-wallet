{
  "root": "build/",
  "routes": {
    "/**": "index.html"
  },
  "proxies": {
    "/doc/": {
      "origin": "http://blog.mibao.net/doc/"
    }
  },
  "headers": {
    "/": {
      "Cache-Control": "no-cache,must-revalidate,proxy-revalidate,max-age=300,stale-while-revalidate=600,stale-if-error=600"
    },
    "/static/**": {
      "Cache-Control": "public, max-age=512000"
    },
    "*.js": {
      "Cache-Control": "public, max-age=512000"
    },
    "*.png": {
      "Cache-Control": "public, max-age=512000"
    }
  }
}
