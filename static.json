{
  "root": "build/",
  "routes": {
    "/**": "index.html"
  },
  "headers": {
    "/": {
      "Cache-Control": "no-cache,must-revalidate,proxy-revalidate,max-age=300"
    },
    "/static/**": {
      "Cache-Control": "public, max-age=512000"
    },
    "*.js": {
      "Cache-Control": "public, max-age=512000"
    },
    "*.png": {
      "Cache-Control": "public, max-age=512000"
    }
  }
}
